<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_team" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="in.ac.iitp.anwesha.Team">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ExpandableListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/elv_team"
            android:groupIndicator="@android:drawable/dark_header"
            android:clipChildren="true"
            >

        </ExpandableListView>






    </LinearLayout>
</RelativeLayout>
